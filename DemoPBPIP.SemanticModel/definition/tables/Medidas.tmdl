	measure 'Gross Sales Total' = SUM ( financials[Gross Sales] )
		lineageTag: 26ba29c9-1dd8-42ec-b290-7daedc0dc1f6

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Profit Total Base' = SUM ( financials[Profit] )
		lineageTag: b91d4c2b-7320-4f1d-96a9-66319b0c5cec

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Precio Promedio' = DIVIDE ( [Ventas Totales], [Unidades Vendidas] )
