	/// Calculates the total sales amount by summing the 'Sales' column from the 'financials' table.
	measure 'Ventas Totales' = SUM ( financials[Sales] )
		lineageTag: 90ecd69b-b52a-4554-8fb9-d1024d7f5660

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	/// Calculates the total cost of goods sold by summing the 'COGS' column from the 'financials' table.
	measure 'Costo Total (COGS)' = SUM ( financials[COGS] )
		lineageTag: 152c0f2a-6a49-4d6c-8251-a8dfe26e9318

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	/// Calculates the total sum of discounts from the 'Discounts' column in the 'financials' table.
	measure 'Descuentos Totales' = SUM ( financials[Discounts] )
		lineageTag: fd0024f0-cb7a-40ff-9cf2-02905ed7481e

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	/// Calculates the total profit by subtracting total cost of goods sold and total discounts from total sales.
	measure 'Beneficio Total' = [Ventas Totales] - [Costo Total (COGS)] - [Descuentos Totales]
		lineageTag: 0617567e-5365-4339-baf3-8350f022d33e

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	/// Calculates the profit margin percentage by dividing total profit by total sales.
	measure 'Margen %' = DIVIDE ( [Beneficio Total], [Ventas Totales] )
